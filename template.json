{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Transform": "AWS::Serverless-2016-10-31",
  "Resources": {
    "LambdaFunction": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Handler": "better-youtube-playlists",
        "Runtime": "go1.x",
        "CodeUri": "./",
        "Policies": ["AWSLamhdaBasicExecutionRole"],
        "Events": {
          "ApiEvent": {
            "Type": "Api",
            "Properties": {
              "Path": "/{proxy+}",
              "Method": "ANY"
            }
          }
        }
      }
    }
  }
}
